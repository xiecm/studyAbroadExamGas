.title {
    font-size: 32rpx;
    line-height: 94rpx;
    font-weight: 700;
    padding: 0 20rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    border-bottom: 4rpx solid #f2f3f7;
}
.head {
    display: flex;
    margin-top: 24rpx;
}
.head-info {
    margin-left: 20rpx;
}
.head-flag-box {
    display: flex;
    font-size: 24rpx;
    line-height: 32rpx;
}
.head-flag {
    background: #ffe3bb;
    border-radius: 6rpx;
    margin-right: 16rpx;
    padding: 0 10rpx;
}
.head-text {
    font-size: 28rpx;
    line-height: 72rpx;
    color: #808080;
}
.head-mark {
    width: 104rpx;
    text-align: center;
    margin: 10rpx 20rpx 0 auto;
}
.head-mark-icon {
    width: 48rpx;
    height: 46rpx;
    margin: 0 auto;
}
.head-mark-text {
    font-size: 20rpx;
    line-height: 34rpx;
}
.cover {
    width: 710rpx;
    height: 430rpx;
    position: absolute;
    top: 234rpx;
    left: 20rpx;
    z-index: 1;
}
.video {
    width: 710rpx;
    height: 430rpx;
    margin: 8rpx auto 0;
    border-radius: 6rpx;
}
.tit1, .tit2 {
    font-size: 28rpx;
    margin-left: 20rpx;
}
.tit1 {
    line-height: 66rpx;
    margin-top: 12rpx;
}
.tit2 {
    line-height: 48rpx;
    margin-top: 16rpx;
}
.intro {
    font-size: 28rpx;
    line-height: 36rpx;
    color: #808080;
    width: 710rpx;
    margin: 0 auto;
    padding-bottom: 22rpx;
    border-bottom: 4rpx solid #f2f3f7;
}
.wrap {
    width: 740rpx;
    margin: 0 auto;
    display: flex;
    flex-flow: row wrap;
    font-size: 28rpx;
    line-height: 52rpx;
    color: #808080;
    text-align: center;
}
.item {
    width: 120rpx;
    border: 1px solid #808080;
    border-radius: 6rpx;
    margin: 10rpx 14rpx;
}
.active {
    color: #fd6102;
    border-color: #fd6102;
}